emacsclient -e '(progn (find-file "~/org/bookmarks.org") (org-html-export-to-html))' \
&& mkdir -p ~/.linkchecker \
&& mv ~/org/bookmarks.html ~/.linkchecker/bookmarks.html \
&& docker run --rm -t -u $(id -u):$(id -g) -v ~/.linkchecker:/mnt linkchecker/linkchecker --threads 1 --check-extern bookmarks.html
